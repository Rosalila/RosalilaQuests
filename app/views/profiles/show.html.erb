<p id="notice"><%= notice %></p>

<p>
  <img src="<%= @profile.picture %>" />
</p>

<p>
  <b>Real name:</b>
  <%= @profile.real_name %>
</p>

<p>
  <b>Nick:</b>
  <%= @profile.nick %>
</p>

<%if current_user.id==@profile.users_id %>
  <%= link_to 'Edit', edit_profile_path(@profile) %> |
  <%= link_to 'Back', profiles_path %>
<%end%>
